{
  "appdir": "{{appdir}}",
  "appversion": "1.0.0.0",

  "server": {
    "ipAddress": "127.0.0.1",
    "tcpPort": 8080,
    "http": {
      "path": "/api/"
    },
    "websocket": {
      "path": "/websocket/"
    },
    "files": {
      "path": "/web/",
      "rootDirectory": "{{appdir}}/web",
      "dirListingEnabled": true
    },
    "rootRedirectLocation": "/web/index.html"
  },

  "web": {
    "webSocketUrl": "ws://127.0.0.1:8080/websocket/"
  },

  "logging": {
    "console": {
      "level": "INFO"
    },
    "file": {
      "level": "DEBUG",
      "maxBytes": 1048576,
      "maxBackupCount": 16,
      "filename": "{{appdir}}/log/example.log",
      "mode": "a",
      "dateFormat": "YYYY-MM-DD HH:mm:ss.SSS"
    }
  },

  "winscm": {
    "name": "example",
    "pluginPath": "{{appdir}}/bin/windows_scm_plugin_1.0.0_deno_1.10.dll"
  },

  "installer": {
    "helpLink": "https://github.com/notranspile-js/deno-vue-example",
    "productName": "Example Application",
    "displayName": "Example Application",
    "description": "Example Application built with Deno and Vue",
    "shortcutLabel": "Example Application",
    "manufacturer": "alex@staticlibs.net",
    "upgradeCode": "08f2045a-87b2-41ce-80a7-4f73db5e5fa8",
    "installDirName": "example",
    "msiFileName": "example",
    "serviceName": "example",
    "pluginDllUrl": "https://github.com/notranspile-js/deno-windows-scm/releases/download/1.0.0_deno_1.10/windows_scm_plugin_1.0.0_deno_1.10.dll"
  }
}
